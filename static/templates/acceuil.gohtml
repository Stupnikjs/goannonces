{{template "base" .}}

{{define "content"}}
    <section class="accueil-section">
         {{ range .Data.Tracks}} 
           <div class="audiocard"> 
              <p>{{ .Name }}</p>
              <button  data-url="/stream/sound/{{ .ID }}">
                {{ .Name }}
              
              </button>
            </div>
         {{ end }}
    </section>
    <script defer src="/static/js/acceuil.js"></script>
{{end}}